  <tr>
    <td>
    <% 
        countValues=0
       params["options"].each_with_index do |type, index| %>
      
        <% optionType = Spree::OptionType.where(:name=>type.to_s.gsub('"', '')).first %>
        <div class="form-group" data-hook="presentation">
          <div class="onclick">Fresh </div>
          <%= f.select :option_types, options_from_collection_for_select(Spree::OptionValue.where(:option_type_id => optionType).order(:name => :asc), "id", "name"), {},:name=> 'variant[][option_types_'+index.to_s+']',:multiple=> false,:class=> 'select2 btn' %>
              <%= f.hidden_field :option_type, name:'option_type', value: optionType.name %>
        </div> 
       
        <% countValues=countValues+1 %>
    <% end %>
     <%= f.hidden_field :price,name: 'countValues', min:0 ,class: 'form-control',id:'variantid', value: countValues %>
    </td>
    <td>
  <div class="form-group">
          <%= f.field_container :quantity, class: ['form-group'] do %>
            <%= f.number_field :quantity,name:"variant[][quantity]",class: 'form-control', value: 1, min:0  %>
          <% end %>
        </div>
</td>

<td>
      <div data-hook="new_product_price">
        <%= f.field_container :price, class: ['form-group'] do %>
          <%= f.label :price, Spree.t('MRP') %> <span class="required">*</span>
<%= f.text_field :mrp,name: "variant[][mrp]",class: 'form-control',id:'product_mrp' %>
<%= f.label :price, Spree.t(:price) %> <span class="required">*</span>
          <%= f.text_field :price,name: "variant[][price]",:step => 50, value: number_to_currency(@product.price, unit: ''), min:0 ,class: 'form-control',id:'first_product product_price' %>
